<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="adminfaq">
	
	
	
	<select id="getFaqList" resultType="faqVo">
		SELECT *
		FROM FAQ
		WHERE STATUS = 'O'
		ORDER BY NO DESC
	</select>
	

</mapper>